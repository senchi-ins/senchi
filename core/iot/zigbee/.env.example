# Google API keys
GOOGLE_SHEETS_API_KEY=

# DB
REDIS_URL=

# Apple notification server URL
APPLE_PUSH_NOTIFICATION_SERVER=api.sandbox.push.apple.com:443
APNS_KEY_ID=
